<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>The Multimedia Terminal</display-name>
  <welcome-file-list>
    <welcome-file>start</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Start</servlet-name>
    <servlet-class>com.themmt.controller.StartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Start</servlet-name>
    <url-pattern>/start</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Search</servlet-name>
    <servlet-class>com.themmt.controller.Search</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Search</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminApproval</servlet-name>
    <servlet-class>com.themmt.controller.AdminApprovalServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminApproval</servlet-name>
    <url-pattern>/adminApproval</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Admin</servlet-name>
    <servlet-class>com.themmt.controller.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/admin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Propose</servlet-name>
    <servlet-class>com.themmt.controller.ProposeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Propose</servlet-name>
    <url-pattern>/propose</url-pattern>
  </servlet-mapping>
</web-app>